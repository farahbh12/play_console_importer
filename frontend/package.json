    "react-router-bootstrap": "0.26.2",
    "react-router-dom": "6.21.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "react-transition-group": "4.4.5",
    "reactstrap": "^8.10.1",
    "sass": "1.69.5",
    "web-vitals": "2.1.4"
